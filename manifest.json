{
  "manifest_version": 2,
  "name": "Kakao Vaccine Script",
  "description": "카카오 잔여백신 예약하기",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Kakao Vaccine"
  },
  "background": {
    "scripts": ["module/jquery.min.js", "js/common.js"]
  },
  "permissions": ["http://www.kma.go.kr/*"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["module/jquery.min.js"]
    }
  ]
}
